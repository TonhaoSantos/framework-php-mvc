# editorconfig.org

root = true

[*]
charset = utf-8
indent_size = 4
indent_style = space
insert_final_newline = true
end_of_line = lf
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.html]
indent_style = tab
indent_size = 4

[*.css]
indent_style = tab
indent_size = 4
 
[scripts/*.js]
indent_style = tab
indent_size = 2